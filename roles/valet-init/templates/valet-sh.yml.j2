hub:
  host: "dummy.example.com"
  port: 22
  path: "/data"

services:
  composer:
    version: 2
  node:
    version: 16
  php:
    version: 8.1
  mariadb:
    version: 10.4
    database: "projectx"
  elasticsearch:
    version: 7
    plugins:
      - "analysis-phonetic"
      - "analysis-icu"
  rabbitmq:
    vhost: project-x

instance:
  key: "projectX"
  type: "magento2"
  path: "src"
  multidomain:
    de: "default"
    en: "en"
  sync:
    identifier: test
    db: true
    fs:
      - "pub/media"
    post_restore_actions:
      indexer:
        reindexAll: true
      commands:
        - bin/magento admin:user:create --admin-user=admin --admin-password=admin123 --admin-email=info@example.com --admin-firstname=Admin --admin-lastname=Admin
  crypt_key: "abcdefghijklmnopqrstuvxyz1234567890crypt"
  config: {}
